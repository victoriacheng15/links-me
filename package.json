{
  "name": "linktree-me",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rome:check": "pnpm exec rome check src/",
    "rome:apply": "pnpm exec rome check src/ --apply",
    "rome:format": "pnpm exec rome format src/",
    "rome:write": "pnpm exec rome format src/ --write",
    "prettier:tw": "prettier src/ --plugin=prettier-plugin-tailwindcss --write"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "rome": "^11.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.2"
  }
}
